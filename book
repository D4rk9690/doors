local ESP = loadstring(game:HttpGet("https://pastebin.com/raw/2tnXa8tZ", true))()

for _, v in next, workspace.CurrentRooms:GetDescendants() do
    if v:FindFirstChild("Base") and string.find(v:GetFullName(), "LiveHintBook") then
          ESP:Add(v.Base, {name = "Hint book", color = Color3.fromRGB(65,105,225)})
    end
end
